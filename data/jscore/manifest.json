{
	"name" : "jasy",

	"fields" :
	{
		"es5" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "jasy.detect.ES5"
		},

		"engine" : 
		{
			"check" : ["webkit","gecko","trident","presto"],
			"detect" : "jasy.detect.Engine"
		},

		"debug" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "jasy.detect.Param"
		},

		"locale" : 
		{
			"check" : "String",
			"default" : "en",
			"detect" : "jasy.detect.Locale"
		}
	}
}
